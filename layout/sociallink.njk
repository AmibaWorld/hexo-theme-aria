{% block sociallink %}
<hr>
<div class="social-links">
  <p><i class="far fa-address-card"></i> Social Links</p>
  <ul>
    {% for k, v in theme.social %}
    <li>{% if v.icon %}<i class="{{ v.icon }}"></i> {% endif %}<a href="{{ v.link }}" target="_blank">{{ k }}</a></li>
    {% endfor %}
  </ul>
</div>
{% endblock %}
